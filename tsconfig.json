{
   "extends": "expo/tsconfig.base",
   "compilerOptions": {
      "strict": true,
      "baseUrl": ".",
      "paths": {
         "@screens/*": ["./src/screens/*"],
         "@components/*": ["./src/components/*"],
         "@models/*": ["./src/models/*"],
         "@typings/*": ["./src/typings/*"],
         "@routes/*": ["./src/routes/*"],
         "@constants/*": ["./src/constants/*"],
         "@utils/*": ["./src/utils/*"],
         "@services/*": ["./src/services/*"]
      }
   }
}
